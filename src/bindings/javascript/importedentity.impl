bool isImport() const
{
    return ImportedEntity::isImport();
}

ImportSourcePtr importSource() const
{
    return ImportedEntity::importSource();
}

void setImportSource(const ImportSourcePtr &importSource)
{
    ImportedEntity::setImportSource(importSource);
}

std::string importReference() const
