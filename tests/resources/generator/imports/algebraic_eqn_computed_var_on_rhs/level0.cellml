<?xml version="1.0" encoding="iso-8859-1"?>

<model name="model_with_import" xmlns="http://www.cellml.org/cellml/2.0#"
	xmlns:cellml="http://www.cellml.org/cellml/2.0#" xmlns:xlink="http://www.w3.org/1999/xlink">
	<import xlink:href="model.cellml">
		<component name="level1_in_level0" component_ref="my_algebraic_eqn" />
	</import>
	<component name="my_algebraic_eqn">
        <variable name="x" units="dimensionless" interface="public"/>
        <variable name="a" units="dimensionless" interface="public"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci>x</ci>
                <ci>a</ci>
            </apply>
		</math>
	</component>
	<connection component_1="my_algebraic_eqn" component_2="level1_in_level0">
		<map_variables variable_1="a" variable_2="a" />
	</connection>
</model>
