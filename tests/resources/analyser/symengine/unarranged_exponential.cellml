<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://www.cellml.org/cellml/2.0#" name="UnarrangedExponentialEquations">
  <component name="Component">

    <!-- Precomputed constants -->
    <variable name="w" units="dimensionless"/>
    <variable name="x" units="dimensionless"/>
    <variable name="y" units="dimensionless"/>
    <variable name="z" units="dimensionless"/>

    <!-- Unknowns -->
    <variable name="a" units="dimensionless"/>
    <variable name="b" units="dimensionless"/>
    <variable name="c" units="dimensionless"/>
    <variable name="d" units="dimensionless"/>

    <math xmlns="http://www.w3.org/1998/Math/MathML"
          xmlns:cellml="http://www.cellml.org/cellml/2.0#">

      <!-- Precomputed constants -->
      <apply><eq/><ci>w</ci><cn cellml:units="dimensionless">2</cn></apply>
      <apply><eq/><ci>x</ci><cn cellml:units="dimensionless">3</cn></apply>
      <apply><eq/><ci>y</ci><cn cellml:units="dimensionless">5</cn></apply>
      <apply><eq/><ci>z</ci><cn cellml:units="dimensionless">7</cn></apply>


      <!-- Equation 1 (unknown a):
           exp(a + w) + 3*exp(a - x) = 200
      -->
      <apply>
        <eq/>
        <apply>
          <plus/>
          <apply><exp/><apply><plus/><ci>a</ci><ci>w</ci></apply></apply>
          <apply><times/><cn cellml:units="dimensionless">3</cn>
            <apply><exp/><apply><minus/><ci>a</ci><ci>x</ci></apply></apply>
          </apply>
        </apply>
        <cn cellml:units="dimensionless">200</cn>
      </apply>


      <!-- Equation 2 (unknown b):
           exp(2*b) - exp(b + y) + x*z = 100
      -->
      <apply>
        <eq/>
        <apply>
          <plus/>
          <apply><minus/>
            <apply><exp/><apply><times/><cn cellml:units="dimensionless">2</cn><ci>b</ci></apply></apply>
            <apply><exp/><apply><plus/><ci>b</ci><ci>y</ci></apply></apply>
          </apply>
          <apply><times/><ci>x</ci><ci>z</ci></apply>
        </apply>
        <cn cellml:units="dimensionless">100</cn>
      </apply>


      <!-- Equation 3 (unknown c):
           exp(c/w + z) + y*exp(c - x) = 500
      -->
      <apply>
        <eq/>
        <apply>
          <plus/>
          <apply><exp/>
            <apply><plus/>
              <apply><divide/><ci>c</ci><ci>w</ci></apply>
              <ci>z</ci>
            </apply>
          </apply>
          <apply><times/><ci>y</ci>
            <apply><exp/><apply><minus/><ci>c</ci><ci>x</ci></apply></apply>
          </apply>
        </apply>
        <cn cellml:units="dimensionless">500</cn>
      </apply>


      <!-- Equation 4 (unknown d):
           4*exp(d - y) + exp( z/(d + w) ) = 300
      -->
      <apply>
        <eq/>
        <apply>
          <plus/>
          <apply><times/><cn cellml:units="dimensionless">4</cn>
            <apply><exp/><apply><minus/><ci>d</ci><ci>y</ci></apply></apply>
          </apply>
          <apply><exp/>
            <apply><divide/><ci>z</ci>
              <apply><plus/><ci>d</ci><ci>w</ci></apply>
            </apply>
          </apply>
        </apply>
        <cn cellml:units="dimensionless">300</cn>
      </apply>

    </math>
  </component>
</model>
