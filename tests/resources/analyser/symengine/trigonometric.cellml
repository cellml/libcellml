<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://www.cellml.org/cellml/2.0#" name="UnarrangedTrigEquations">
  <component name="Component">
    <!-- Precomputed constants -->
    <variable name="w" units="radian"/>
    <variable name="x" units="radian"/>
    <variable name="y" units="radian"/>
    <variable name="z" units="radian"/>

    <!-- Unknowns to solve -->
    <variable name="a" units="radian"/>
    <variable name="b" units="radian"/>
    <variable name="c" units="radian"/>
    <variable name="d" units="radian"/>

    <math xmlns="http://www.w3.org/1998/Math/MathML"
          xmlns:cellml="http://www.cellml.org/cellml/2.0#">

      <!-- Initialize constants -->
      <apply><eq/><ci>w</ci><cn cellml:units="radian">0.5</cn></apply>
      <apply><eq/><ci>x</ci><cn cellml:units="radian">1.0</cn></apply>
      <apply><eq/><ci>y</ci><cn cellml:units="radian">1.5</cn></apply>
      <apply><eq/><ci>z</ci><cn cellml:units="radian">2.0</cn></apply>

      <!-- Equation 1: 2*a + sin(w) = 1 -->
      <apply>
        <eq/>
        <apply><plus/><apply><times/><cn cellml:units="dimensionless">2</cn><ci>a</ci></apply><apply><sin/><ci>w</ci></apply></apply>
        <cn cellml:units="dimensionless">1</cn>
      </apply>

      <!-- Equation 2: cos(x + 4) - b = 0 -->
      <apply>
        <eq/>
        <apply><minus/><apply><cos/><apply><plus/><cn cellml:units="dimensionless">4</cn><ci>x</ci></apply></apply><ci>b</ci></apply>
        <cn cellml:units="dimensionless">0</cn>
      </apply>

      <!-- Equation 3: tan(c) = z -->
      <apply>
        <eq/>
        <apply><tan/><ci>c</ci></apply>
        <ci>z</ci>
      </apply>

      <!-- Equation 4: d - sin(y - x) = 2 -->
      <apply>
        <eq/>
        <apply><minus/><ci>d</ci><apply><sin/><apply><minus/><ci>y</ci><ci>x</ci></apply></apply></apply>
        <cn cellml:units="dimensionless">2</cn>
      </apply>

    </math>
  </component>
</model>
