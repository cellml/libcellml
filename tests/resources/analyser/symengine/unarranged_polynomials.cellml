<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://www.cellml.org/cellml/2.0#" name="UnarrangedPolynomialEquationsOneSolution">
  <component name="Component">
    <variable name="a" units="dimensionless"/>
    <variable name="b" units="dimensionless"/>
    <variable name="c" units="dimensionless"/>

    <math xmlns="http://www.w3.org/1998/Math/MathML"
          xmlns:cellml="http://www.cellml.org/cellml/2.0#">

      <!-- Equation 1: (a + 2)^2 = 25  --> 
      <!-- Unique solution: a = 3 -->
      <apply>
        <eq/>
        <apply>
          <power/>
          <apply><plus/><ci>a</ci><cn cellml:units="dimensionless">2</cn></apply>
          <cn cellml:units="dimensionless">2</cn>
        </apply>
        <cn cellml:units="dimensionless">25</cn>
      </apply>

      <!-- Equation 2: b^3 + 4 = 12 -->
      <!-- Unique solution: b = 2 -->
      <apply>
        <eq/>
        <apply>
          <plus/>
          <apply><power/><ci>b</ci><cn cellml:units="dimensionless">3</cn></apply>
          <cn cellml:units="dimensionless">4</cn>
        </apply>
        <cn cellml:units="dimensionless">12</cn>
      </apply>

      <!-- Equation 3: c^3 + 3*c = 30 -->
      <!-- Unique solution: b = 2 -->
      <apply>
        <eq/>
        <apply>
          <plus/>
          <apply><power/><ci>c</ci><cn cellml:units="dimensionless">3</cn></apply>
          <apply><times/><cn cellml:units="dimensionless">3</cn><ci>c</ci></apply>
        </apply>
        <cn cellml:units="dimensionless">30</cn>
      </apply>

    </math>
  </component>
</model>
