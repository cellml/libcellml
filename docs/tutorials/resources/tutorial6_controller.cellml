<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://www.cellml.org/cellml/2.0#" name="controllers">
  <units name="millisecond">
    <unit prefix="milli" units="second"/>
  </units>
  <units name="millivolt">
    <unit prefix="milli" units="volt"/>
  </units>
  <component name="controller">
    <variable name="t" units="millisecond" interface="public_and_private"/>
    <variable name="V" units="millivolt" interface="public_and_private"/>
    <math xmlns="http://www.w3.org/1998/Math/MathML"
      xmlns:cellml="http://www.cellml.org/cellml/2.0#">
      <apply>
        <eq/>
        <ci>V</ci>
        <piecewise>
          <piece>
            <cn cellml:units="millivolt">0</cn>
            <apply>
              <lt/>
              <ci>t</ci>
              <cn cellml:units="millisecond">5</cn>
            </apply>
          </piece>
          <piece>
            <cn cellml:units="millivolt">0</cn>
            <apply>
              <gt/>
              <ci>t</ci>
              <cn cellml:units="millisecond">15</cn>
            </apply>
          </piece>
          <otherwise>
            <cn cellml:units="millivolt">-85</cn>
          </otherwise>
        </piecewise>
      </apply>
    </math>
  </component>
  <component name="parameters">
    <variable name="E_K" units="millivolt" initial_value="-85" interface="public_and_private"/>
    <variable name="g_K" units="milliS_per_cm2" initial_value="36" interface="public_and_private"/>
    <variable name="n" units="dimensionless" initial_value="0.325" interface="public_and_private"/>
  </component>
</model>
