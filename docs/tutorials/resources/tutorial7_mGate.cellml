<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://www.cellml.org/cellml/2.0#" name="mGateModel">
  <units name="mV">
    <unit prefix="milli" units="volt"/>
  </units>
  <units name="ms">
    <unit prefix="milli" units="second"/>
  </units>
  <units name="per_ms">
    <unit exponent="-1" prefix="milli" units="second"/>
  </units>
  <units name="per_mV_ms">
    <unit exponent="-1" units="ms"/>
    <unit exponent="-1" units="mV"/>
  </units>
  <component name="mGate">
    <variable name="V" units="mV" interface="public"/>
    <variable name="t" units="ms" interface="public"/>
    <variable name="alpha_m" units="per_ms"/>
    <variable name="beta_m" units="per_ms"/>
    <variable name="m" units="dimensionless" interface="public_and_private"/>
    <math xmlns="http://www.w3.org/1998/Math/MathML"
      xmlns:cellml="http://www.cellml.org/cellml/2.0#">
      <apply>
        <eq/>
        <ci>alpha_m</ci>
        <apply>
          <divide/>
          <apply>
            <times/>
            <apply>
              <minus/>
              <cn cellml:units="per_mV_ms">0.1</cn>
            </apply>
            <apply>
              <plus/>
              <ci>V</ci>
              <cn cellml:units="mV">50</cn>
            </apply>
          </apply>
          <apply>
            <minus/>
            <apply>
              <exp/>
              <apply>
                <divide/>
                <apply>
                  <minus/>
                  <apply>
                    <plus/>
                    <ci>V</ci>
                    <cn cellml:units="mV">50</cn>
                  </apply>
                </apply>
                <cn cellml:units="mV">10</cn>
              </apply>
            </apply>
            <cn cellml:units="dimensionless">1</cn>
          </apply>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>beta_m</ci>
        <apply>
          <times/>
          <cn cellml:units="per_ms">4</cn>
          <apply>
            <exp/>
            <apply>
              <divide/>
              <apply>
                <minus/>
                <apply>
                  <plus/>
                  <ci>V</ci>
                  <cn cellml:units="mV">75</cn>
                </apply>
              </apply>
              <cn cellml:units="mV">18</cn>
            </apply>
          </apply>
        </apply>
      </apply>
      <apply>
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci>t</ci>
          </bvar>
          <ci>m</ci>
        </apply>
        <apply>
          <minus/>
          <apply>
            <times/>
            <ci>alpha_m</ci>
            <apply>
              <minus/>
              <cn cellml:units="dimensionless">1</cn>
              <ci>m</ci>
            </apply>
          </apply>
          <apply>
            <times/>
            <ci>beta_m</ci>
            <ci>m</ci>
          </apply>
        </apply>
      </apply>
    </math>
  </component>

</model>