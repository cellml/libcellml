<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://www.cellml.org/cellml/2.0#" name="Tutorial8_LeakageCurrentModel">
  <import xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="tutorial8_leakageInit.cellml">
    <component component_ref="leakage_initialiser" name="leakageInit"/>
  </import>
  <units name="mV">
    <unit prefix="milli" units="volt"/>
  </units>
  <units name="ms">
    <unit prefix="milli" units="second"/>
  </units>
  <units name="per_mV">
    <unit exponent="-1" prefix="milli" units="volt"/>
  </units>
  <units name="mS_per_cm2">
    <unit prefix="milli" units="siemens"/>
    <unit exponent="-2" prefix="centi" units="metre"/>
  </units>
  <units name="microA_per_cm2">
    <unit prefix="micro" units="ampere"/>
    <unit exponent="-2" prefix="centi" units="metre"/>
  </units>
  <component name="leakageCurrent">
    <variable name="i_L" units="microA_per_cm2" interface="public" />
    <variable name="g_L" units="mS_per_cm2" interface="private"/>
    <variable name="E_L" units="mV" interface="private"/>
    <variable name="V" units="mV" interface="public" />
    <math xmlns="http://www.w3.org/1998/Math/MathML"
      xmlns:cellml="http://www.cellml.org/cellml/2.0#">
      <apply>
        <eq/>
        <ci>i_L</ci>
        <apply>
          <times/>
          <ci>g_L</ci>
          <apply>
            <minus/>
            <ci>V</ci>
            <ci>E_L</ci>
          </apply>
        </apply>
      </apply>
    </math>
  </component>
  <connection component_1="leakageCurrent" component_2="leakageInit">
    <map_variables variable_1="E_L" variable_2="E_L"/>
    <map_variables variable_1="g_L" variable_2="g_L"/>
  </connection>
  <encapsulation>
    <component_ref component="leakageCurrent">
      <component_ref component="leakageInit"/>
    </component_ref>
  </encapsulation>
</model>
